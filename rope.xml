            <inertial pos="0 0 0" quat="1 0 0 0" mass="0.1" diaginertia="0.001 0.001 0.001"/>
            <composite type="cable" curve="cos(s) sin(s)" count="25 1 1" size="1 0.22 1">
                <plugin plugin="mujoco.elasticity.cable">
                    <config key="twist" value="1e7"/>
                    <config key="bend" value="4e6"/>

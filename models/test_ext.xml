<mujoco model="computer_rack">
    <worldbody>
        <body name="base" pos="0 0 0">
            <body name="arm" pos="0.5 0 0">
                <joint name="joint1" type="hinge" pos="-0.5 0 0" axis="0 1 0" limited="true" range="-180 180" damping="10" frictionloss="0.1"/>
                <site name="sensor_site" pos="0 0 0"/>
                <geom type="box" name="arm" pos="0 0 0" size="0.5 0.02 0.02" rgba="1 0.1 0.1 1"/>
            </body>
        </body>
        <body name="force_tester" pos="0.9 0 0.1">
            <inertial pos="0 0 0" mass="0.01" diaginertia="0.0001 0.0001 0.0001"/>
            <joint type="slide" name="slider" axis="0 0 1" stiffness="50" springref="-0.1" damping="1"/>
            <geom type="box" name="force_tester" size="0.08 0.08 0.08" rgba="0.1 1 0.1 1"/>
        </body>
    </worldbody>
    <actuator>
        <intvelocity name="vel" joint="joint1" ctrllimited="true" ctrlrange="-0.45 0.45" actrange="-6.2 6.2"
                     kp="100000" forcelimited="true" forcerange="-20 20"/>
    </actuator>
    <sensor>
        <torque name="torque" site="sensor_site"/>
        <force name="force" site="sensor_site"/>
    </sensor>
</mujoco>

<mujoco model="untangle">
    <default>
        <joint stiffness="0.5"/>
        <geom friction="0.01"/>
    </default>
    <option viscosity="1"/>
    <include file="materials.xml"/>
    <include file="val_husky.xml"/>
    <include file="computer_rack.xml"/>
    <include file="floor.xml"/>
    <include file="rope.xml"/>
    <include file="cc_sphere.xml"/>
    <worldbody>
        <body name="mycamera">
            <camera mode="fixed" name="mycamera" xyaxes="0.2 -1 0 0.5 0 0.5" pos="-0.3 -0.5 2.0" />
        </body>
    </worldbody>
    <equality>
        <connect body1="attach" body2="B_first" name="attach" anchor="0 0 0" active="true" />
        <connect body1="drive50" body2="B_first" name="left" anchor="0 0 0.181" active="false" solref="0.03 1"/>
        <connect body1="drive10" body2="B_last" name="right" anchor="0 0 0.181" active="false" solref="0.03 1"/>
    </equality>
</mujoco>

<mujoco model="conq">
    <compiler angle="radian" meshdir="meshes/" inertiafromgeom="true"/>
    <option viscosity="1.0" timestep="0.004"/>
    <worldbody>
        <body name="conq_base">
            <body name="conq_body">
                <geom name="conq_base" type="box" pos="0 0 0.35" size="0.4 0.15 0.07" rgba="0.8 0.8 0.2 1"/>
                <geom name="front_left_leg" type="box" pos="0.4 0.15 0.2" size="0.01 0.01 0.2" rgba="0 0 0 1"/>
                <geom name="front_right_leg" type="box" pos="0.4 -0.15 0.2" size="0.01 0.01 0.2" rgba="0 0 0 1"/>
                <geom name="back_left_leg" type="box" pos="-0.4 0.15 0.2" size="0.01 0.01 0.2" rgba="0 0 0 1"/>
                <geom name="back_right_leg" type="box" pos="-0.4 -0.15 0.2" size="0.01 0.01 0.2" rgba="0 0 0 1"/>
                <inertial pos="0 0 0" quat="1 0 0 0" mass="1"/>
                <joint name="hand_x" type="slide" axis="1 0 0" pos="0 0 0" damping="400"/>
                <joint name="hand_y" type="slide" axis="0 1 0" pos="0 0 0" damping="400"/>
                <body name="hand" pos="0.6 0 0.4" quat="1 0 0 0">
                    <inertial pos="0 0 0" quat="1 0 0 0" mass="3"/>
                    <geom name="hand" type="box" size="0.02 0.03 0.04" rgba="0.9 0.7 0.2 1"/>
                    <site name="hand_tool" pos="0.03 0 -0.03"/>
                    <joint name="hand_z" type="slide" axis="0 0 1" pos="0 0 0" damping="10"/>
                    <body name="finger" pos="0.05 0 0" quat="1 0 0 0">
                        <inertial pos="0 0 0" quat="1 0 0 0" mass="0.2"/>
                        <geom name="finger" type="box" pos="0 0 0.01" size="0.005 0.03 0.03" rgba="0.1 0.1 0.1 1"/>
                        <joint name="finger" type="hinge" axis="0 -1 0" pos="0 0 0.04" damping="1"/>
                    </body>
                </body>
            </body>
        </body>
    </worldbody>
    <actuator>
        <intvelocity name="hand_x_vel" joint="hand_x" ctrllimited="true" ctrlrange="-0.5 0.5" kp="1000" actrange="-2 2" forcelimited="true" forcerange="-1000 1000"/>
        <intvelocity name="hand_y_vel" joint="hand_y" ctrllimited="true" ctrlrange="-0.5 0.5" actrange="-2 2" kp="1000" forcelimited="true" forcerange="-1000 1000"/>
        <intvelocity name="hand_z_vel" joint="hand_z" ctrllimited="true" ctrlrange="-0.5 0.5" actrange="-0.35 0.5" kp="200" forcelimited="true" forcerange="-50 50"/>
        <intvelocity name="finger_vel" joint="finger" ctrllimited="true" ctrlrange="-1.5707 1.5707" actrange="0.0 1.5707" kp="10" forcelimited="true" forcerange="-1 1"/>
    </actuator>
</mujoco>

<mujoco model="drones">
    <compiler angle="radian" meshdir="meshes/" inertiafromgeom="true"/>
    <option viscosity="0.1" timestep="0.005"/>
    <include file="materials.xml"/>
    <asset>
        <mesh name="tree" file="tree.stl"/>
        <mesh name="drone_frame" file="drone_frame.stl"/>
        <mesh name="claw" file="claw.stl"/>
    </asset>
    <default>
        <geom friction="1"/>
        <joint damping="1.0"/>
        <default class="rope">
            <joint type="ball" damping="1.0" stiffness="1.0"/>
            <geom size="0.05 0.543182" pos="0.543182 0 0" quat="0.707107 0 -0.707107 0" type="capsule"
                  rgba="0.3 0.2 0.2 1"/>
        </default>
        <default class="visual">
            <!--             any geom with the class "visual" will not collide with anything -->
            <geom contype="0" conaffinity="0" group="2"/>
        </default>
    </default>
    <worldbody>
        <!-- used for recording what happens in the simulation -->
        <body name="mycamera">
            <camera name="mycamera" pos="24.827 -9.392 32.070" xyaxes="0.281 0.960 -0.000 -0.761 0.223 0.609"/>
        </body>
        <body name="mycamera2">
            <camera name="mycamera2" pos="-29.825 -20.569 18.945" xyaxes="0.441 -0.897 0.000 0.250 0.123 0.960"/>
        </body>
        <light name="top" pos="0 -1 50" mode="trackcom" castshadow="false"/>
        <body name="obstacles">
            <body pos="0 0 -0.445" name="ground">
                <geom type="plane" name="ground" size="150 150 0.01" material="grass"/>
                <body name="attach1" pos="0 -20 0.2"/>
                <body name="attach2" pos="0 20 0.2"/>
            </body>
            <body name="tree" pos="8 -10 0" euler="0 0 0.8">
                <geom name="tree" type="mesh" mesh="tree" pos="0 0 -0.5" material="light-wood"/>
                <body name="branch" euler="0 0 2.356">
                    <site name="branch0" pos="0 0 -0.5"/>
                    <site name="branch1" pos="0 0 4"/>
                    <site name="branch2" pos="2 -0.3 5.5"/>
                    <site name="branch3" pos="5 0.4 7"/>
                    <site name="branch4" pos="9.7 1 7"/>
                    <site name="branch5" pos="9.7 1 -0.5"/>
                </body>
            </body>
        </body>
        <body name="drones" pos="0 0 11.7">
            <body name="drone1" pos="0 -4 0">
                <geom name="drone1_body" type="mesh" mesh="drone_frame"/>
                <body name="drone1_rope" pos="0 0 -0.1" quat="0.707141 0 0.707073 0">
                    <body name="drone1_B_first">
                        <joint name="drone1_J_first" class="rope"/>
                        <geom name="drone1_G0" class="rope"/>
                        <body name="drone1_B_1" pos="1.08 0 0">
                            <joint name="drone1_J_1" class="rope"/>
                            <geom name="drone1_G1" class="rope"/>
                            <body name="drone1_B_2" pos="1.08 0 0">
                                <joint name="drone1_J_2" class="rope"/>
                                <geom name="drone1_G2" class="rope"/>
                                <body name="drone1_B_3" pos="1.08 0 0">
                                    <joint name="drone1_J_3" class="rope"/>
                                    <geom name="drone1_G3" class="rope"/>
                                    <body name="drone1_B_4" pos="1.08 0 0">
                                        <joint name="drone1_J_4" class="rope"/>
                                        <geom name="drone1_G4" class="rope"/>
                                        <body name="drone1_B_5" pos="1.08 0 0">
                                            <joint name="drone1_J_5" class="rope"/>
                                            <geom name="drone1_G5" class="rope"/>
                                            <body name="drone1_B_6" pos="1.08 0 0">
                                                <joint name="drone1_J_6" class="rope"/>
                                                <geom name="drone1_G6" class="rope"/>
                                                <body name="drone1_B_7" pos="1.08 0 0">
                                                    <joint name="drone1_J_7" class="rope"/>
                                                    <geom name="drone1_G7" class="rope"/>
                                                    <body name="drone1_B_8" pos="1.08 0 0">
                                                        <joint name="drone1_J_8" class="rope"/>
                                                        <geom name="drone1_G8" class="rope"/>
                                                        <body name="drone1_B_9" pos="1.08 0 0">
                                                            <joint name="drone1_J_9" class="rope"/>
                                                            <geom name="drone1_G9" class="rope"/>
                                                            <body name="drone1_claw" pos="1.0 0 0" euler="1.5707 0 0">
                                                                <site name="drone1_claw" pos="1 0 0"/>
                                                                <geom name="drone1_claw" type="mesh" mesh="claw"/>
                                                            </body>
                                                        </body>
                                                    </body>
                                                </body>
                                            </body>
                                        </body>
                                    </body>
                                </body>
                            </body>
                        </body>
                    </body>
                </body>
            </body>
            <body name="drone2" pos="0 4 0">
                <geom name="drone2_body" type="mesh" mesh="drone_frame"/>
                <body name="drone2_rope" pos="0 0 -0.1" quat="0.707141 0 0.707073 0">
                    <body name="drone2_B_first">
                        <joint name="drone2_J_first" class="rope"/>
                        <geom name="drone2_G0" class="rope"/>
                        <body name="drone2_B_1" pos="1.08 0 0">
                            <joint name="drone2_J_1" class="rope"/>
                            <geom name="drone2_G1" class="rope"/>
                            <body name="drone2_B_2" pos="1.08 0 0">
                                <joint name="drone2_J_2" class="rope"/>
                                <geom name="drone2_G2" class="rope"/>
                                <body name="drone2_B_3" pos="1.08 0 0">
                                    <joint name="drone2_J_3" class="rope"/>
                                    <geom name="drone2_G3" class="rope"/>
                                    <body name="drone2_B_4" pos="1.08 0 0">
                                        <joint name="drone2_J_4" class="rope"/>
                                        <geom name="drone2_G4" class="rope"/>
                                        <body name="drone2_B_5" pos="1.08 0 0">
                                            <joint name="drone2_J_5" class="rope"/>
                                            <geom name="drone2_G5" class="rope"/>
                                            <body name="drone2_B_6" pos="1.08 0 0">
                                                <joint name="drone2_J_6" class="rope"/>
                                                <geom name="drone2_G6" class="rope"/>
                                                <body name="drone2_B_7" pos="1.08 0 0">
                                                    <joint name="drone2_J_7" class="rope"/>
                                                    <geom name="drone2_G7" class="rope"/>
                                                    <body name="drone2_B_8" pos="1.08 0 0">
                                                        <joint name="drone2_J_8" class="rope"/>
                                                        <geom name="drone2_G8" class="rope"/>
                                                        <body name="drone2_B_9" pos="1.08 0 0">
                                                            <joint name="drone2_J_9" class="rope"/>
                                                            <geom name="drone2_G9" class="rope"/>
                                                            <body name="drone2_claw" pos="1.0 0 0" euler="1.5707 0 0">

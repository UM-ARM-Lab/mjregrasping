                                                            </body>
                                                        </body>
                                                    </body>
                                                </body>
                                            </body>
                                        </body>
                                    </body>
                                </body>
                            </body>
                        </body>
                    </body>
                </body>
            </body>
            <body name="drone3" pos="0 12 0">
                <geom name="drone3_body" type="mesh" mesh="drone_frame"/>
                <body name="drone3_rope" pos="0 0 -0.1" quat="0.707141 0 0.707073 0">
                    <body name="drone3_B_first">
                        <joint name="drone3_J_first" class="rope"/>
                        <geom name="drone3_G0" class="rope"/>
                        <body name="drone3_B_1" pos="1.08 0 0">
                            <joint name="drone3_J_1" class="rope"/>
                            <geom name="drone3_G1" class="rope"/>
                            <body name="drone3_B_2" pos="1.08 0 0">
                                <joint name="drone3_J_2" class="rope"/>
                                <geom name="drone3_G2" class="rope"/>
                                <body name="drone3_B_3" pos="1.08 0 0">
                                    <joint name="drone3_J_3" class="rope"/>
                                    <geom name="drone3_G3" class="rope"/>
                                    <body name="drone3_B_4" pos="1.08 0 0">
                                        <joint name="drone3_J_4" class="rope"/>
                                        <geom name="drone3_G4" class="rope"/>
                                        <body name="drone3_B_5" pos="1.08 0 0">
                                            <joint name="drone3_J_5" class="rope"/>
                                            <geom name="drone3_G5" class="rope"/>
                                            <body name="drone3_B_6" pos="1.08 0 0">
                                                <joint name="drone3_J_6" class="rope"/>
                                                <geom name="drone3_G6" class="rope"/>
                                                <body name="drone3_B_7" pos="1.08 0 0">
                                                    <joint name="drone3_J_7" class="rope"/>
                                                    <geom name="drone3_G7" class="rope"/>
                                                    <body name="drone3_B_8" pos="1.08 0 0">
                                                        <joint name="drone3_J_8" class="rope"/>
                                                        <geom name="drone3_G8" class="rope"/>
                                                        <body name="drone3_B_9" pos="1.08 0 0">
                                                            <joint name="drone3_J_9" class="rope"/>
                                                            <geom name="drone3_G9" class="rope"/>
                                                            <body name="drone3_claw" pos="1.0 0 0" euler="1.5707 0 0">
                                                                <site name="drone3_claw" pos="1 0 0"/>
                                                                <geom name="drone3_claw" type="mesh" mesh="claw"/>
                                                            </body>
                                                        </body>
                                                    </body>
                                                </body>
                                            </body>
                                        </body>
                                    </body>
                                </body>
                            </body>
                        </body>
                    </body>
                </body>
            </body>
        </body>
        <body pos="0 -15 -0.25" euler="0 0 1.5707" name="rope">
            <inertial pos="0 0 0" quat="1 0 0 0" mass="1" diaginertia="0.001 0.001 0.001"/>
            <composite type="cable" curve="s" count="30 1 1" size="30 0.22 1">
                <joint kind="main" damping="1"/>
                <geom type="capsule" size="0.2" rgba=".7 .4 .1 1"/>
            </composite>
            <joint type="free"/>
        </body>
    </worldbody>
    <equality>
        <connect name="drone1" body1="drone1_claw" body2="B_11" solref="0.01 1" anchor="1 0 0" active="true"/>
        <connect name="drone2" body1="drone2_claw" body2="B_19" solref="0.01 1" anchor="1 0 0" active="true"/>
        <connect name="drone3" body1="drone3_claw" body2="B_25" solref="0.01 1" anchor="1 0 0" active="true"/>
    </equality>
</mujoco>


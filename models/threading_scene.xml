<mujoco model="untangle">
    <compiler angle="radian" meshdir="meshes/" inertiafromgeom="true"/>
    <asset>
    </asset>
    <default>
        <joint stiffness="0.5"/>
        <geom friction="0.01"/>
    </default>
    <option viscosity="1" timestep="0.005"/>
    <include file="materials.xml"/>
    <include file="val_husky.xml"/>
    <include file="floor.xml"/>
    <include file="rope.xml"/>
    <include file="cc_sphere.xml"/>
    <include file="val_eq_constraints.xml"/>
    <worldbody>
        <body name="cable_tube" pos="0.8 0.0 0.0">
            <geom name="cable_tube_bottom" type="box" size="0.027 0.05 0.002" pos="0 0 0"/>
            <geom name="cable_tube_top" type="box" size="0.027 0.05 0.002" pos="0 0 0.05"/>
            <geom name="cable_tube_front" type="box" size="0.002 0.05 0.027" pos="-0.025 0 0.025"/>
            <geom name="cable_tube_back" type="box" size="0.002 0.05 0.027" pos="0.025 0 0.025"/>
        </body>
        <body name="attach" pos="0 0.25 0.55">
            <geom type="box" name="attach" size="0.005 0.005 0.005" rgba="0.5 0 0 1"/>
        </body>
    </worldbody>
    <equality>
        <connect body1="attach" body2="B_first" name="attach" anchor="0 0 0" active="false" solref="0.03 1"/>
    </equality>
    <worldbody>
        <!-- used for recording what happens in the simulation -->
        <body name="mycamera">
            <camera mode="fixed" name="mycamera" xyaxes="0.2 -1 0 0.5 0 0.5" pos="-0.3 -0.5 2.0"/>
        </body>
    </worldbody>
</mujoco>

<mujoco model="threading_scene">
    <default>
        <joint stiffness="0.5"/>
        <geom friction="0.01"/>
        <default class="visual">
            <!--             any geom with the class "visual" will not collide with anything -->
            <geom contype="0" conaffinity="0" group="2"/>
        </default>
    </default>
    <include file="materials.xml"/>
    <worldbody>
        <!-- used for recording what happens in the simulation -->
        <body name="mycamera">
            <camera mode="fixed" name="mycamera" xyaxes="1.0 0.4 0 0.0 1.0 0.5" pos="0.5 -0.1 1.7"/>
        </body>
        <light name="top" pos="-1 0 105" mode="trackcom"/>
    </worldbody>
    <include file="val.xml"/>
    <include file="floor.xml"/>
    <include file="threading_obstacles.xml"/>
    <include file="cable.xml"/>
    <equality>
        <weld body1="drive50" body2="B_first" name="left" relpose="0 0 0.181 1 0 0 0" active="false" solref="0.04 1" torquescale="0.2"/>
        <weld body1="drive10" body2="B_first" name="right" relpose="0 0 0.181 1 0 0 0" active="false" solref="0.04 1" torquescale="0.2"/>

        <connect body1="attach1" body2="B_first" name="attach1" anchor="0 -0.03 0" active="false" solref="0.03 1"/>
        <connect body1="attach2" body2="B_first" name="attach2" anchor="0 0.03 0" active="false" solref="0.03 1"/>
        <!-- NOTE: body2 can and will be changed at runtime -->
        <connect body1="loop1" body2="B_first" name="attach_loop1" anchor="0 0 0.06" active="false" solref="0.03 1"/>
        <connect body1="loop2" body2="B_first" name="attach_loop2" anchor="0 0 0.06" active="false" solref="0.03 1"/>
        <connect body1="loop3" body2="B_first" name="attach_loop3" anchor="0 0 0.06" active="false" solref="0.03 1"/>
    </equality>
</mujoco>

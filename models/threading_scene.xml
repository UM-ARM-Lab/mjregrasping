<mujoco model="untangle">
    <compiler angle="radian" meshdir="meshes/" inertiafromgeom="true"/>
    <asset>
    </asset>
    <default>
        <joint stiffness="0.5"/>
        <geom friction="0.01"/>
    </default>
    <option viscosity="1" timestep="0.005"/>
    <include file="materials.xml"/>
    <include file="val_husky.xml"/>
    <include file="floor.xml"/>
    <include file="stiff_wire.xml"/>
    <include file="cc_sphere.xml"/>
    <worldbody>
        <body name="cable_tube" pos="0.9 0.0 0.0">
            <geom name="cable_tube_bottom" type="box" size="0.042 0.08 0.002" pos="0 0 0"/>
            <geom name="cable_tube_top" type="box" size="0.042 0.08 0.002" pos="0 0 0.08"/>
            <geom name="cable_tube_front" type="box" size="0.002 0.08 0.042" pos="-0.04 0 0.04"/>
            <geom name="cable_tube_back" type="box" size="0.002 0.08 0.042" pos="0.04 0 0.04"/>
            <site name="cable_tube_center" pos="0 0 0.04"/>
        </body>
        <body name="attach" pos="0 0.25 0.55">
            <geom type="box" name="attach" size="0.005 0.005 0.005" rgba="0.5 0 0 1"/>
        </body>
    </worldbody>
    <worldbody>
        <!-- used for recording what happens in the simulation -->
        <body name="mycamera">
            <camera mode="fixed" name="mycamera" xyaxes="0.2 -1 0 0.5 0 0.5" pos="-0.3 -0.5 2.0"/>
        </body>
    </worldbody>
    <equality>
        <weld body1="drive50" body2="B_first" name="left" anchor="0 0 0.17" active="false" solref="0.04 1"/>
        <weld body1="drive10" body2="B_last" name="right" anchor="0 0 0.17" active="false" solref="0.04 1"/>
        <connect body1="drive50" body2="world" name="left_world" anchor="0 0 0.17" active="false" solref="0.015 1"/>
        <connect body1="drive10" body2="world" name="right_world" anchor="0 0 0.17" active="false" solref="0.015 1"/>
        <connect body1="B_last" body2="world" name="rope_world" anchor="0 0 0" active="false" solref="0.1 1"/>
    </equality>
</mujoco>
